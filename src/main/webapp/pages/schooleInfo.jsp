<script language =javascript >
    var curIndex=0;
    //时间间隔 单位毫秒
    var timeInterval=3000;
    var arr=new Array(); //定义一个数组
    arr[0]="img1.jpg";
    arr[1]="img2.jpg";
    arr[2]="img3.jpg";
    arr[3]="img4.jpg";

    setInterval(changeImg,timeInterval);
    function changeImg()
    {
        var obj=document.getElementById("showpic");
        if (curIndex==arr.length-1)
        {
            curIndex=0;
        }
        else
        {
            curIndex+=1;
        }
        obj.src="${pageContext.request.contextPath}/images/"+arr[curIndex];
    }
</script>